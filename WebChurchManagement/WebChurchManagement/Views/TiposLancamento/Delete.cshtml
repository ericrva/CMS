@model WebChurchManagement.Models.TipoLancamento

@{
    ViewBag.Title = "Deletar Tipo de Lançamento";
}

<h3>Tem certeza de que desejar deletar esse registro?</h3>
<div>
    <dl class="dl-horizontal">

        <dt>@Html.DisplayNameFor(model => model.Id_Tpo_Lanc)</dt>
        <dd>@Html.DisplayFor(model => model.Ativo)</dd>
        <dt>@Html.DisplayNameFor(model => model.Desc_Lanc)</dt>

    </dl>

    @using (Html.BeginForm("Deletar", "TiposLancamento", new { id = Model.Id_Tpo_Lanc }, FormMethod.Post))
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Deletar" class="btn btn-danger" /> |
            @Html.ActionLink("Voltar à lista", "Index")
        </div>
    }
</div>
